{
    "compilerOptions": {
      "lib": [ "dom", "es5", "es2015.promise", "es6" ],
      "module": "commonjs",
      "target": "es5",
      "allowJs": true,
      "experimentalDecorators": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "outDir": "dist"
    },
    "include": [
      "src/*"
    ],
    "exclude": [
        "dist"
    ]
  }